# Collide Docker 环境变量配置示例
# 复制此文件为 docker.env 并根据实际环境修改配置

# 数据库配置
MYSQL_HOST=192.168.1.107
MYSQL_PORT=3306
MYSQL_DATABASE=collide
MYSQL_USERNAME=root
MYSQL_PASSWORD=123456

# Redis配置
REDIS_HOST=192.168.1.107
REDIS_PORT=6379  
REDIS_PASSWORD=123456

# Nacos配置
NACOS_URL=192.168.1.107:8848
NACOS_USERNAME=nacos
NACOS_PASSWORD=nacos

# Elasticsearch配置
ELASTICSEARCH_ENABLE=true
ELASTICSEARCH_URL=192.168.1.107:9200
ELASTICSEARCH_USERNAME=elastic
ELASTICSEARCH_PASSWORD=123456

# XXL-Job配置
XXL_JOB_URL=192.168.1.107:8081
XXL_JOB_APP_NAME=xxl-job-executor
XXL_JOB_ACCESS_TOKEN=default_token

# Sentinel配置
SENTINEL_URL=192.168.1.107
SENTINEL_PORT=8888
SENTINEL_NACOS_DATA_ID=collide-gateway-sentinel

# RocketMQ配置
ROCKETMQ_URL=192.168.1.107:9876

# Dubbo配置
DUBBO_NACOS_NAMESPACE=99d4c548-be12-4eaa-9564-4c7921bd80c1
DUBBO_NACOS_GROUP=dubbo

# Seata配置
SEATA_NACOS_DATA_ID=seataServer.properties
SEATA_NACOS_GROUP=seata
SEATA_NACOS_NAMESPACE=806e806f-e916-411c-b4ba-9682ae44b47e

# 服务端口配置（使用host网络时这些端口会直接映射到宿主机）
GATEWAY_PORT=9501
AUTH_PORT=9502
APPLICATION_PORT=9503

# JVM参数配置
GATEWAY_JAVA_OPTS=-Xms256m -Xmx512m -server
AUTH_JAVA_OPTS=-Xms256m -Xmx512m -server
APPLICATION_JAVA_OPTS=-Xms512m -Xmx1024m -server

# 日志级别
LOG_LEVEL=INFO
APP_LOG_LEVEL=DEBUG

# 时区设置
TZ=Asia/Shanghai

# Docker镜像版本
IMAGE_VERSION=latest 