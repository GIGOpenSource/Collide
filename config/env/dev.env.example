# Collide 开发环境配置示例
# 复制此文件为 .env.dev 并根据实际情况修改配置

SPRING_PROFILES_ACTIVE=dev

# 服务端口配置
SERVER_PORT_GATEWAY=9501
SERVER_PORT_AUTH=9502
SERVER_PORT_BUSINESS=9503

# 数据库配置
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DATABASE=collide_dev
MYSQL_USERNAME=root
MYSQL_PASSWORD=123456

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=123456
REDIS_DATABASE=0

# Nacos配置
NACOS_SERVER_ADDR=localhost:8848
NACOS_USERNAME=nacos
NACOS_PASSWORD=nacos

# Dubbo配置
DUBBO_NACOS_NAMESPACE=dev
DUBBO_NACOS_GROUP=dubbo

# Seata配置
SEATA_NACOS_NAMESPACE=dev

# ElasticSearch配置
ES_ENABLED=false
ES_HOST=localhost:9200
ES_USERNAME=elastic
ES_PASSWORD=123456

# RocketMQ配置
ROCKETMQ_NAMESRV_ADDR=localhost:9876

# XXL-Job配置
XXL_JOB_ADMIN_ADDR=localhost:8081
XXL_JOB_ACCESS_TOKEN=default_token

# Sentinel配置
SENTINEL_HOST=localhost
SENTINEL_PORT=8888

# Sa-Token配置 - 开发环境宽松配置
SA_TOKEN_NAME=satoken
SA_TOKEN_TIMEOUT=7200
SA_TOKEN_ACTIVE_TIMEOUT=3600
SA_TOKEN_IS_CONCURRENT=true
SA_TOKEN_IS_SHARE=true
SA_TOKEN_STYLE=uuid
SA_TOKEN_IS_LOG=true

# 业务配置
FILE_UPLOAD_PATH=/tmp/dev-uploads/
FILE_MAX_SIZE=52428800 