# Collide 测试环境配置示例
# 复制此文件为 .env.test 并根据实际情况修改配置

SPRING_PROFILES_ACTIVE=test

# 服务端口配置
SERVER_PORT_GATEWAY=8080
SERVER_PORT_AUTH=8081
SERVER_PORT_BUSINESS=8082

# 数据库配置
MYSQL_HOST=test-mysql.collide.com
MYSQL_PORT=3306
MYSQL_DATABASE=collide_test
MYSQL_USERNAME=collide_test
MYSQL_PASSWORD=change_me_test_password

# Redis配置
REDIS_HOST=test-redis.collide.com
REDIS_PORT=6379
REDIS_PASSWORD=change_me_redis_test_password
REDIS_DATABASE=1

# Nacos配置
NACOS_SERVER_ADDR=test-nacos.collide.com:8848
NACOS_USERNAME=nacos_test
NACOS_PASSWORD=change_me_nacos_test_password

# Dubbo配置
DUBBO_NACOS_NAMESPACE=test
DUBBO_NACOS_GROUP=dubbo

# Seata配置
SEATA_NACOS_NAMESPACE=test

# ElasticSearch配置
ES_ENABLED=true
ES_HOST=test-es.collide.com:9200
ES_USERNAME=elastic
ES_PASSWORD=change_me_es_test_password

# RocketMQ配置
ROCKETMQ_NAMESRV_ADDR=test-rocketmq.collide.com:9876

# XXL-Job配置
XXL_JOB_ADMIN_ADDR=test-xxljob.collide.com:8081
XXL_JOB_ACCESS_TOKEN=change_me_xxljob_test_token

# Sentinel配置
SENTINEL_HOST=test-sentinel.collide.com
SENTINEL_PORT=8888

# Sa-Token配置 - 测试环境配置
SA_TOKEN_NAME=satoken
SA_TOKEN_TIMEOUT=86400
SA_TOKEN_ACTIVE_TIMEOUT=7200
SA_TOKEN_IS_CONCURRENT=false
SA_TOKEN_IS_SHARE=true
SA_TOKEN_STYLE=uuid
SA_TOKEN_IS_LOG=true

# 业务配置
FILE_UPLOAD_PATH=/data/test-uploads/
FILE_MAX_SIZE=20971520

# Druid监控配置
DRUID_ADMIN_USERNAME=admin
DRUID_ADMIN_PASSWORD=change_me_druid_password 